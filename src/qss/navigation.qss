QFrame#Navigation {
    background-color: transparent;
}

QFrame#Navigation > QPushButton {
    color: var(--default-color1);
    background-color: transparent;
    border: none;
}

QFrame#Navigation > QPushButton:hover {
    color: var(--hover-color1);
}

QFrame#Navigation > QPushButton:pressed {
    color: var(--main-color1);
}

QFrame#Navigation > QLabel#L_Appicon {
    font-family: remixicon;
    padding-bottom: 15;
    color: var(--header1-color1);
}
